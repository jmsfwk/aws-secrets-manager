{
    "name": "jmsfwk/aws-secrets-manager",
    "description": "Library for caching AWS Secrets Manager values",
    "keywords": ["AWS", "secrets", "secrets-manager"],
    "type": "library",
    "authors": [
        {
            "name": "James Fenwick",
            "email": "j.fenwick@me.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3.0",
        "aws/aws-sdk-php": "^2|^3",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "jmsfwk\\AwsSecretsManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\AwsSecretsManager\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "lock": false
    }
}
